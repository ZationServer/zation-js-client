{
  "name": "zation-client",
  "version": "1.2.01",
  "license": "MIT",
  "description": "Application Server Framework JavaScript Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "gulp build",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZationServer/zation-js-client.git"
  },
  "author": {
    "name": "Luca Scarignella",
    "email": "lucacode1@gmail.com",
    "url": "https://luca.scaringella.de"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "esay",
    "application",
    "websocket",
    "http",
    "fast",
    "client",
    "realtime",
    "cluster",
    "scalable",
    "secure",
    "big",
    "tools"
  ],
  "dependencies": {
    "lodash": ">=4.17.13",
    "lodash.template": ">=4.5.0",
    "minimatch": "3.0.4",
    "path": "0.12.7",
    "socketcluster-client": "14.3.1",
    "sc-errors": "2.0.1",
    "clone": "2.1.2",
    "linked-list": "2.0.0",
    "object-path": "0.11.4",
    "fast-stringify": "2.0.0",
    "component-emitter": "1.2.1",
    "tiny-emitter": "2.1.0",
    "forint": "0.1.8"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.2",
    "ascjsify": "1.0.0",
    "babel-cli": "7.0.0-beta.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-minify-dead-code-elimination": "0.4.3",
    "browserify": "16.2.3",
    "browserify-ignore-code": "0.1.0",
    "cached-path-relative": "1.0.2",
    "chai": "4.2.0",
    "extend": ">=3.0.2",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "gulp-clean": "0.4.0",
    "gulp-convert-newline": "0.0.3",
    "gulp-derequire": "3.0.0",
    "gulp-insert": "0.5.0",
    "gulp-optimize-js": "1.1.0",
    "gulp-rename": "1.3.0",
    "gulp-terser": "1.2.0",
    "gulp-typescript": "5.0.0-alpha.3",
    "gulp-uglify": "3.0.0",
    "mocha": "7.1.2",
    "nyc": "15.0.1",
    "sinon": "7.2.2",
    "ts-node": "^8.0.3",
    "typescript": "3.5.3",
    "uglify-es": "3.3.9",
    "vinyl-source-stream": "2.0.0"
  }
}
